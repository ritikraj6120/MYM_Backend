{
	"version": 2,
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"headers": [
		{
			"source": "/signin",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "*"
				}
			]
		},
		{
			"source": "/signup",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "*"
				}
			]
		},
		{
			"source": "/",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "*"
				}
			]
		}
	]
}